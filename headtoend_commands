# blablabla
router ip  172.20.10.1

cd /root
sudo modprobe -r iwlwifi mac80211
sudo modprobe iwlwifi connector_log=0x1
sudo killall wpa_supplicant

# connect to a WIFI
sudo linux-80211n-csitool-supplementary/netlink/log_to_file /root/Wifi-Localization-master/sample_data/csi_1122.dat
(ping 172.20.10.1)


# start pycharm
cd /pycharm-community-2017.2.4/bin
./pycharm.sh

